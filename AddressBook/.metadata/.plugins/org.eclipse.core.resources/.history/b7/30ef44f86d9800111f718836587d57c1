				CantactCollection cantactCollection = (CantactCollection)theEObject;
				T result = caseCantactCollection(cantactCollection);
				if (result == null) result = defaultCase(theEObject);
				return result;
			}
			default: return defaultCase(theEObject);
		}
	}

