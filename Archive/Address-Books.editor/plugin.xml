<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="default_.presentation.DefaultModelWizardID"
            name="%_UI_DefaultModelWizard_label"
            class="default_.presentation.DefaultModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/DefaultModelFile.gif">
         <description>%_UI_DefaultModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="default_.presentation.DefaultEditorID"
            name="%_UI_DefaultEditor_label"
            icon="icons/full/obj16/DefaultModelFile.gif"
            extensions="default"
            class="default_.presentation.DefaultEditor"
            contributorClass="default_.presentation.DefaultActionBarContributor">
      </editor>
   </extension>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="default_.package_.presentation.PackageModelWizardID"
            name="%_UI_PackageModelWizard_label"
            class="default_.package_.presentation.PackageModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/PackageModelFile.gif">
         <description>%_UI_PackageModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="default_.package_.presentation.PackageEditorID"
            name="%_UI_PackageEditor_label"
            icon="icons/full/obj16/PackageModelFile.gif"
            extensions="package"
            class="default_.package_.presentation.PackageEditor"
            contributorClass="default_.package_.presentation.PackageActionBarContributor">
      </editor>
   </extension>

</plugin>
